{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debuguj bieżący plik",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "--no-warnings"
      ],
      "outputCapture": "std"
    }
  ]
}
